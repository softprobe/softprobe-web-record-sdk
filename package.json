{
  "name": "@softprobe/softprobe-web-record-sdk",
  "version": "0.0.22",
  "type": "module",
  "main": "dist/softprobe-web-record-sdk.umd.js",
  "module": "dist/softprobe-web-record-sdk.es.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/softprobe/softprobe-web-record-sdk.git"
  },
  "files": [
    "dist/**/**",
    "docs/**/**",
    "!**/*.spec.*",
    "!**/*.json",
    "!**/*.tsbuildinfo",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc  --build tsconfig.sdk.json",
    "build:demo": "vite build --mode=demo && tsc",
    "build:demo-cdn": "npm run build && npm run build:demo && node copy-file.js",
    "preview": "vite preview --mode=demo",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "devDependencies": {
    "@rrweb/types": "^2.0.0-alpha.18",
    "@types/node": "^22.9.0",
    "prettier": "^3.3.3",
    "typescript": "~5.6.2",
    "vite": "^5.4.10"
  },
  "dependencies": {
    "rrweb": "2.0.0-alpha.4",
    "ua-parser-js": "^2.0.2"
  }
}
